{
    "_version": "1.30.0",
    "sap.app": {
        "id": "MyApp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "@sap/ux-app-migrator:freestyle",
            "version": "1.12.1",
            "toolsId": "940e1875-98fb-4031-98f4-3ecfc3558bcf"
        },
        "dataSources": {
            "POService": {
                "uri": "/sap/opu/odata/sap/Z_SAMPLE_ODATA_SRV",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_belize",
            "sap_fiori_3"
        ]
    },
    "sap.ui5": {
        "flexEnabled": true,
        "rootView": {
            "viewName": "MyApp.view.App",
            "type": "XML"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "MyApp.view",
                "controlId": "app",
                "controlAggregation": "pages"
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "Home",
                    "target": "home"
                },
                {
                    "pattern": "PO/{PONumber}",
                    "name": "PurchaseOrder",
                    "target": "PO"
                }                
            ],
            "targets": {
                "home": {
                    "viewName": "POs",
                    "viewId": "poList"
                },
                "PO": {
                    "viewName": "PODetail",
                    "viewId": "poDetail"
                }                
            }
        },     
        "dependencies": {
            "minUI5Version": "1.71.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "models": {
            "": {
                "dataSource": "POService",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "MyApp.i18n.i18n"
                }
            }
        }
    }
}